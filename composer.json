{
  "name": "freebuu/laravel-filterable",
  "type": "package",
  "version": "1.0.2",
  "description": "Model filters for your index requests",
  "keywords": [
    "laravel",
    "laravel filter",
    "laravel filterable",
    "request filter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim",
      "email": "freebuu.dev@gmail.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0 | ^10.0 | ^11.0 | ^12.0",
    "illuminate/database": "^9.0 | ^10.0 | ^11.0 | ^12.0",
    "illuminate/http": "^9.0 | ^10.0 | ^11.0 | ^12.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.54",
    "orchestra/testbench": "^10.0",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^12.0 "
  },
  "autoload": {
    "psr-4": {
      "FreeBuu\\LaravelFilterable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
